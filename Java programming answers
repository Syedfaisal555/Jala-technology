System.out.print("Array b: ");       printArray(b);
// Copy array by iterating
int[] c = {1, 2, 3};
int[] d = new int[c.length];
for (int i = 0; i < d.length;i++){d[i] = c[i];}
d[0]++);
System.out.println("Scenario 2: ");    System.out.print("Array c: ");       printArray(c);
System.out.print("Array d: ");     printArray(d);
   
#7
int[] my_array = {25, 14, 56, 15, 36, 56, 77, 18, 29, 49};
//element in 3rd position of the array 
(index->2, value->5);
int Index_position = 2;
int newValue = 5;
System.out.println("Original Array : "+Arrays.toString(my_array)); 
for(int i=my_array.length-1);
i > Index_position;
i--){ my_array[i] = my_array[i-1];} my_array[Index_position] = newValue; System.out.println("New Array: "+Arrays.toString(my_array));

#8
int temp;
//sort the array 
for (int i = 0; i < a.length; i++) {
for (int j = i + 1; j < a.length; j++) {         if (a[i] > a[j]){
temp = a[i];
a[i] = a[j];
a[j] = temp;}}}
//return second largest element 
return a[a.length - 2];
